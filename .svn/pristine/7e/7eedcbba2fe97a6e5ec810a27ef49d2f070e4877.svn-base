	.LoyaltyLoginDetailsRow{margin-top:5px; color:#585858;}
	#visLtyLogin_txtUserName, #visLtyLogin_txtPassword{margin-top:5px;}
	#visLtyLogin_lblLoginHeader{color:#585858!important;}
	
	#content a{color:#34A9DC;}
	#content a:hover{text-decoration:underline;}
	
	#orderDetails, #tblTotal, #tblClock{margin-bottom:20px;}
	
	#AreaSeats1{position:relative;}
	
	#lblLtyTicHeader{display:block; margin-bottom:10px;}
	#lblLtyTicBlurb{display:block; margin-bottom:30px;}
	#visLtyLogin_lblLoginHeader{display:block; margin-bottom:10px;}
	#lblAboveTicketDetail{display:block; margin-bottom:10px;}
	
	.mb30{margin-bottom:30px;}
	
	#login_left{width:500px; float:left;}
	
	#login_right{width:360px; float:right;}
	
